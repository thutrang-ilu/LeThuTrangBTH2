@model IEnumerable<LeThuTrangBTH2.Models.Person>
<p>
    <a class="btn btn-success" asp-action="Create">Create a new Person</a>
</p>
<table class="table table-all">
    <tr>
        <th>PersonID</th>
        <th>PersonName</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(m => item.PersonID)</td>
            <td>@Html.DisplayFor(m => item.PersonName)</td>
        </tr>
    }
</table>